
page {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	box-sizing: border-box;
	background-color: #efeff4
}
view {
	font-size: 28rpx;
	line-height: inherit
}
.example {
	padding: 0 30rpx 30rpx
}
.example-title {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	font-size: 32rpx;
	color: #464e52;
	padding: 30rpx 30rpx 30rpx 50rpx;
	margin-top: 20rpx;
	position: relative;
	background-color: #fdfdfd;
	border-bottom: 1px #f5f5f5 solid
}
.example-title__after {
	position: relative;
	color: #031e3c
}
.example-title:after {
	content: '';
	position: absolute;
	left: 30rpx;
	margin: auto;
	top: 0;
	bottom: 0;
	width: 6rpx;
	height: 32rpx;
	background-color: #ccc
}
.example .example-title {
	margin: 40rpx 0
}
.example-body {
	padding: 30rpx;
	background: #fff
}
.example-info {
	padding: 30rpx;
	color: #3b4144;
	background: #fff
}
.example-body {
	padding: 30rpx 0;
}
.example-box {
	margin-bottom: 30rpx;
}
.example-box:last-child {
	margin-bottom: 0;
}
.image-box {
	width: 100%;
	height: 350rpx;
	overflow: hidden;
}
.image-box .image {
	width: 100%;
	height: 100%;
}
.content-box {
	padding-top: 20rpx;
}
.footer-box {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	width: 100%;
}
.footer-box__item {
	width: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.footer-box__item:nth-child(2) {
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.footer-box__item:last-child {
	-webkit-box-pack: end;
	-webkit-justify-content: flex-end;
	        justify-content: flex-end;
}

